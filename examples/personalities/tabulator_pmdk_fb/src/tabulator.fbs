// Example tabulator
namespace Proto;

struct Value {
  min: float;
  max: float;
  mean: float;
}

table UpdateRequest {
  sample: float;
}

table UpdateReply {
  status: int;
}

table QueryRequest {
  key: string;
}

table QueryReply {
  status: int;
  value: Value;
}

union Element
{
  UpdateRequest,
  UpdateReply, 
  QueryRequest,
  QueryReply, 
}

table Message
{
  element : Element;
}

root_type Message;
